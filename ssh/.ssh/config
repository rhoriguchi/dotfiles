Host gitlab.com
    IdentityFile ~/.ssh/gitlab_rsa
    User git

Host hsr-server
    Hostname sinv-56016.edu.hsr.ch
    User root

Host *
    AddressFamily any
    Compression yes
    ConnectionAttempts 3
    # Necessary for zsh completion
    HashKnownHosts no
    IdentityFile ~/.ssh/id_rsa
    NumberOfPasswordPrompts 3
    AddKeysToAgent yes
    PubkeyAuthentication yes
