Host *
    AddressFamily any
    Compression yes
    ConnectionAttempts 3
    # Necessary for zsh completion
    HashKnownHosts no
    IdentityFile ~/.ssh/id_rsa
    NumberOfPasswordPrompts 3
    AddKeysToAgent yes
    RSAAuthentication yes
    PubkeyAuthentication yes

